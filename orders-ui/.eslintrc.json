{  "extends": [
    "next/core-web-vitals"
  ],  "rules": {
    "react/jsx-uses-react": "off",
    "react/react-in-jsx-scope": "off",
    "react/prop-types": "off",
    "react-hooks/exhaustive-deps": "warn",
    
    "prefer-const": "error",
    "no-var": "error",
    "no-console": ["warn", { "allow": ["warn", "error"] }],
    "eqeqeq": "error"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
